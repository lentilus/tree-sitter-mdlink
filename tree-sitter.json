{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mdlink",
      "camelcase": "mdlink",
      "title": "mdlink",
      "scope": "source.mdlink",
      "file-types": [
        "mdlink"
      ],
      "injection-regex": "^mdlink$",
      "class-name": "TreeSitterMdlink"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "a simple grammar for detecting markdown style links",
    "authors": [
      {
        "name": "lentilus"
      }
    ],
    "links": {
      "repository": "https://github.com/lentilus/tree-sitter-mdlink"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
