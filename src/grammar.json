{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mdlink",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_inline"
      }
    },
    "_inline": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "link"
        },
        {
          "type": "PATTERN",
          "value": "[^\\[\\]\\(\\)\\n]+"
        }
      ]
    },
    "link": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "PATTERN",
            "value": "[^\\]\\n]+"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "destination",
          "content": {
            "type": "PATTERN",
            "value": "[^)\\n]+"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}